.nursing .perline view{
  border-radius: 6rpx;
  margin-right: 6rpx;
}
.nursing .waterline .perline view:first-child{
  width: calc(33.33% - 3rpx);
  background: #F3EDDE;
}
.nursing .waterline .perline view:nth-child(2){
  width: calc(16.67% - 3rpx);
  background: #E8DBBE;
}
.nursing .waterline .perline view:nth-child(3){
  width: calc(16.67% - 3rpx);
  background: #E8DBBE;
}
.nursing .waterline .perline view:nth-child(4){
  width: calc(33.33% - 3rpx);
  background: #D9C392;
  margin-right: 0;
}
.nursing .waterline .pernum view:last-child{
  width: 20rpx;
}

/* 油分 */
/* 弹性 */
.nursing .otherline .perline view:first-child{
  width: calc(25% - 6rpx);
  background: #F3EDDE;
}
.nursing .otherline .perline view:nth-child(2){
  width: calc(50% - 6rpx);
  background: #E8DBBE;
}
.nursing .otherline .perline view:nth-child(3){
  width: 25%;
  background: #D9C392;
  margin-right: 0;
}






.nursing .result_per .perline .perball{
  position: absolute;
  left: 0%;
  top: -10rpx;
  width: 20rpx;
  height: 20rpx;
  border-radius: 50%;
  border: 4rpx solid #ffffff;
  box-shadow: 0px 0px 8px 0px rgba(217, 195, 146, 1);
  background: #D9C392;
}

.nursing .perstate{
  width: 100%;
  display: flex;
  font-size: 26rpx;
  color: #A7A5A6;
  line-height: 38rpx;
  margin-top: 15rpx;
  height: auto !important;
}
.nursing .perstate view{
  width: 100%;
  background: none !important;
  text-align: center;
}
.end_nursing{
  padding-bottom: 30rpx;
}