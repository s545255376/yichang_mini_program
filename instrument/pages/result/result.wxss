page{
  background: #ffffff;
}
.basic{
  width: calc(100% - 40rpx);
  padding: 40rpx 20rpx 60rpx;
  display: flex;
}
.basic .part{
  margin: 0 10rpx;
  background: #F8F8F8;
  width: calc(25% - 20rpx);
  text-align: center;
  padding: 34rpx 0;
  border-radius: 20rpx;
}
.msg{
  font-size: 32rpx;
  line-height: 45rpx;
  color: #231F20;
}
.title{
  font-size: 26rpx;
  line-height: 40rpx;
  margin-top: 10rpx;
  color: #A7A5A6;
}

.nursing{
  width: calc(100% - 60rpx);
  padding: 0 30rpx;
}
.nursing .title{
  line-height: 54rpx;
  height: 54rpx;
  color: #231F20;
  font-size: 36rpx;
  font-weight: bold;
}
.nursing .title image{
  width: 54rpx;
  margin-right: 16rpx;
  vertical-align: middle;
}
.nursing .info{
  width: calc(100% - 60rpx);
  padding: 30rpx;
  margin: 30rpx 0 60rpx;
  background: #F8F8F8;
  border-radius: 20rpx;
  color: #686868;
  font-size: 30rpx;
  line-height: 45rpx;
}
.nursing .info .result{
  margin: 30rpx 0;
}
.nursing .result_num{
  font-size: 30rpx;
  line-height: 44rpx;
  color: #231F20;
  font-weight: bold;
  margin-bottom: 30rpx;
}
.nursing .result_num text{
  font-size: 44rpx;
  margin-left: 12rpx;
  margin-right: 20rpx;
}
.nursing .result_num view{
  display: inline-block;
  color: #ffffff;
  font-size: 26rpx;
  line-height: 38rpx;
  height: 38rpx;
  background: #676767;
  padding: 0 14rpx;
  border-radius: 19rpx;
}

.nursing .pernum{
  width: 100%;
  position: relative;
  height: 38rpx !important;
  padding-bottom: 15rpx;
}
.nursing .pernum view{
  background: none !important;
  text-align: left;
  font-size: 26rpx;
  line-height: 38rpx;
  color: #A7A5A6;
  margin-left: -10rpx;
}
.nursing .pernum view:first-child{
  margin-left: 0rpx;
}
