} */
.nursing .result_per .pernum view:first-child{
  text-align: left;
}
.nursing .result_per .pernum view:last-child{
  text-align: right;
}
.nursing .pernum view:last-child{
  width: 20rpx;
}
.nursing .result_per .perline{
  width: 100%;
  height: 12rpx;
  border-radius: 6rpx;
  position: relative;
  margin: 15rpx 0;
  display: flex;
}
