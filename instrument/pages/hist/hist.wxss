.ic_check{
  position: fixed;
  right: 32rpx;
  bottom: 40rpx;
  text-align: center;
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
  color: #ffffff;
  background: #231F20;
  box-shadow: 0px 5px 12px 0px rgba(0, 0, 0, 0.3);
  opacity: 0.6;
}
.ic_check image{
  width: 52rpx;
  margin-top: 15rpx;
}
.ic_check view{
  font-size: 24rpx;
  line-height: 24rpx;
  display: block;
}

/* 日期区间选择 */
.time_picker{
  /* position: fixed;
  left: 0;
  bottom: 0;
  background: #ffffff;
  width:100%; */
  position: relative;
  left: 0;
  top: 0;
  z-index: 999;
  background: #ffffff;
  border-radius: 0 0 45rpx 45rpx;
  box-shadow: 0 12rpx 14rpx rgba(0, 0, 0, 0.05);
}
.time_picker .month{
  width: 100%;
  padding: 40rpx 0 0;
  color: #231F20;
  font-size: 30rpx;
  text-align: center;
}
.time_picker .month image{
  width: 40rpx;
  vertical-align: middle;
  margin: 0 30rpx;
}
.time_picker .account-box{
  width: 100%;
}
.time_picker .account-box .f-rili-head {
	width: 100%;
  display: flex;
  padding: 42rpx 0 10rpx;
}
.time_picker .account-box .f-rili-th {
	width: 14.25%;
	text-align: center;
	line-height: 24rpx;
	height: 24rpx;
  color: #918F8F;
	font-size: 24rpx;
}
.time_picker .account-box .f-tbody {
	width: auto;
	height: auto;
  background: #FFFFFF;
  padding-top: 20rpx;
}
.time_picker .account-box .f-td {
	display: inline-block;
	width: 14.25%;
	text-align: center;
	height: 46rpx;
	line-height: 46rpx;
	position: relative;
  margin-top: 32rpx;
  font-size: 30rpx;
}
.time_picker .account-box .datenum {
	border-radius: 50%;
	width: 30rpx;
	height: 30rpx;
	line-height: 30rpx;
	text-align: center;
  margin: 0 auto;
  color: #231F20;
  text-align: center;
  padding: 10rpx;
}
.time_picker .account-box .datebg .dom{
  width: 10rpx;
  height: 10rpx;
  background: #D9C392;
  border-radius: 50%;
  margin: 10rpx auto 0;
}
.time_picker .account-box .unclick{
  color: #DBDBE2;
}
