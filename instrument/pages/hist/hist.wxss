page{
  background: #ffffff;
}
.head{
  width: calc(100% - 60rpx);
  background: #ffffff;
  padding: 40rpx 30rpx 0;
  position: relative;
  z-index: 999;
}
.head image{
  width: 40rpx;
  margin: auto 0;
  float: right;
  vertical-align: middle;
}
.check_state{
  width: calc(100% - 80rpx);
  display: inline-block;
}
.check_state view{
  width: fit-content;
  border-radius: 54rpx;
  text-align: center;
  font-size: 30rpx;
  line-height: 30rpx;
  padding: 12rpx 24rpx;
  color: #A7A5A6;
  display: inline-block;
  margin-right: 30rpx;
}
.check_state .checked{
  background:#231F20;
  color: #ffffff;
}
.list{
  width: calc(100% - 40rpx);
  padding: 50rpx 20rpx;
}
.list .date{
  text-align: right;
  line-height: 80rpx;
  border-top: 1px solid #E8E8E8;
  width: calc(100% - 60rpx);
  margin: 0 30rpx;
  font-size: 24rpx;
  color: #A7A5A6;
}
.list .part{
  background: #F8F8F8;
  border-radius: 20rpx;
  width: 100%;
  margin-bottom: 20rpx;
}
.list .part_info{
  width: calc(100% - 60rpx);
  padding: 50rpx 30rpx;
  display: flex;
  position: relative;
}
.list .part_info .state{
  position: absolute;
  left: 0;
  top: 0;
  width: 56rpx;
}
.list .part_info .image{
  width: 98rpx;
  height: 98rpx;
  border-radius: 50%;
  border: 3rpx solid #D9C392;
  text-align: center;
  font-size: 22rpx;
  line-height: 33rpx;
  color: #D9C392;
  margin: auto 0;
}
.list .part_info .image image{
  width: 58rpx;
  margin: 0 auto;
  display: block;
}
.list .part_info .info{
  width: calc(100% - 160rpx);
  margin-left: 45rpx;
  text-align: center;
}
.list .part_info .info view{
  width: 100%;
  display: flex;
}
.list .part_info .info .title{
  line-height: 36rpx;
  font-size: 24rpx;
  color: #A7A5A6;
}
.list .part_info .info .pernum{
  font-size: 30rpx;
  color: #231F20;
  line-height: 40rpx;
  margin-top: 26rpx;
}
.list .part_info .info .perline{
  width: 100%;
  height: 12rpx;
  border-radius: 6rpx;
  position: relative;
  display: flex;
  background: linear-gradient(90deg, #F6E6DA 0%, #D9C392 100%);
}
.list .part_info .info .perline .perball{
  position: absolute;
  left: 0%;
  top: -10rpx;
  width: 20rpx;
  height: 20rpx;
  border-radius: 50%;
  border: 4rpx solid #ffffff;
  box-shadow: 0px 0px 8px 0px rgba(217, 195, 146, 1);
  background: #D9C392;
}
