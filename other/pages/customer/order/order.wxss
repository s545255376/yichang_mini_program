/* 日期区间选择 */
.time_picker{
  position: fixed;
  left: 55rpx;
  top: 50%;
  z-index: 999;
  background: #ffffff;
  width: calc(100% - 110rpx);
  margin-top: -400rpx;
  border-radius: 20rpx;
}
.time_picker .month{
  width: calc(100% - 80rpx);
  padding: 40rpx;
  color: #231F20;
  font-size: 30rpx;
}
.time_picker .month view{
  float: right;
}
.time_picker .month image{
  width: 40rpx;
  margin-left: 20rpx;
}
.time_picker .btn{
  border-top: 1px solid #E7E7E7;
  padding: 35rpx 45rpx;
  text-align: right;
}
.time_picker .btn view{
  width: 200rpx;
  height: 70rpx;
  line-height: 70rpx;
  text-align: center;
  font-size: 24rpx;
  display: inline-block;
  border-radius: 5rpx;
}
.time_picker .btn .cancel{
  color: #B8BFC6;
}
.time_picker .btn .confirm{
  background: #C2253A;
  color: #ffffff;
}
.time_picker .account-box{
  width: 100%;
}
.time_picker .account-box .f-rili-head {
	width: 100%;
	display: flex;
  border-bottom: 1px solid #EEEEEE;
}
.time_picker .account-box .f-rili-th {
	width: 14.25%;
	text-align: center;
	line-height: 60rpx;
	height: 60rpx;
  background-color: #fff;
  color: #EEA1AC;
	font-size: 26rpx;
}
.time_picker .account-box .f-tbody {
	width: auto;
	height: auto;
  background: #FFFFFF;
  padding-top: 20rpx;
	padding-bottom: 30rpx;
}
.time_picker .account-box .f-td {
	display: inline-block;
  width: 14.25%;
	text-align: center;
	height: 60rpx;
	line-height: 60rpx;
	position: relative;
  font-size: 26rpx;
  margin-top: 10rpx;
}
.time_picker .account-box .datenum {
	border-radius: 50%;
	width: 60rpx;
	height: 60rpx;
	line-height: 60rpx;
	text-align: center;
  margin: 0 auto;
  color: #000000;
}
.time_picker .account-box .clickdom{
  color: #ffffff;
  background: #C2253A;
}
.time_picker .account-box .unclick{
  color: #C0C0C0;
}
.time_picker .account-box .datebg{
  background: rgba(194, 37, 58, 0.06);
}
.time_picker .account-box .datebg_start{
  border-radius: 30rpx 0 0 30rpx;
  background:none;
  background-image: linear-gradient(to right, #ffffff 50% , rgba(194, 37, 58, 0.06) 50%);
}
.time_picker .account-box .datebg_end{
  border-radius: 0 30rpx 30rpx 0;
  background:none;
  background-image: linear-gradient(to right, rgba(194, 37, 58, 0.06) 50%, #ffffff 50% );
}