.head{
  width: 100%;
  height: 200rpx;
  position: fixed;
  left: 0;
  top: 0;
}
.head image{
  width: 100%;
  height: 100%;
}
.head .info{
  position: absolute;
  left: 0;
  top: 55rpx;
  width: 100%;
  height: auto;
}
.head .info view{
  width: 100%;
  height: auto;
  text-align: center;
}
.head .num{
  font-size: 50rpx;
  line-height: 50rpx;
  color: #ffffff;
}
.head .tips{
  color:#FFFFFF;
  opacity: 0.6;
  font-size: 24rpx;
  line-height: 24rpx;
  margin-top: 16rpx;
}
.tabbar{
  top: 200rpx;
  color: #7B7979;
}
/* part */
.page{
  margin-top: 284rpx;
}
.part{
  width: 100%;
}
.part .month{
  width: calc(100% - 60rpx);
  padding: 20rpx 30rpx;
  font-size: 24rpx;
  line-height: 36rpx;
  color: #A7A5A6;
  background: #F8F8F9;
}
.part .list{
  width: calc(100% - 60rpx);
  padding: 0 30rpx;
  background: #ffffff;
  margin-bottom: 10rpx;
}
.part .list:last-child{
  margin-bottom: 0rpx;
}
