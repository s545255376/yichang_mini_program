.part .list_order{
  padding: 30rpx 0; 
  width: 100%;
  border-bottom: 1rpx solid #F8F8F9;
  display: flex;
}
.part .order_num{
  width: calc(100% - 200rpx);
  color: #7B7979;
  font-size: 28rpx;
  line-height: 40rpx;
}
.part .num{
  width: 200rpx;
  color: #231F20;
  font-size: 30rpx;
  text-align: right;
  margin: auto;
}
.part .addnum{
  color: #F1AFAD !important;
}
.part .list_info{
  width: 70%;
  padding: 30rpx 0;
}
.part .list_info view{
  width: 100%;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
  font-size: 26rpx;
  color: #231F20;
  line-height: 40rpx;
  margin-bottom: 10rpx;
}
.part .list_info view:last-child{
  margin-bottom: 0rpx;
}
.part .date{
  font-size: 26rpx;
  color: #A7A5A6;
  line-height: 40rpx;
  padding-bottom: 30rpx;
}
.ic_empty{
  position: relative;
  margin-top: 100rpx;
}
/* 积分说明 */
.intro{
  width: calc(100% - 60rpx);
  padding: 50rpx 30rpx 0 30rpx;
  margin-top: 20rpx;
  background: #ffffff;
}
.intro view{
  padding-bottom: 50rpx;
  font-size: 30rpx;
  color: #7B7979;
  text-align: justify;
}