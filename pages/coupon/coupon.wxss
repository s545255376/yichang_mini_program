.part .part_l {
    width: 27.5%;
    height: auto;
    text-align: center;
    padding: 44rpx 0;
    position: relative;
    border-top: 1px solid #231F20;
    border-left: 1px solid #231F20;
    border-bottom: 1px solid #231F20;
    border-right: 1px dotted #231F20;
    display: flex;
}

.part .part_l::after {
    content: '';
    width: 26rpx;
    height: 26rpx;
    background: #ffffff;
    position: absolute;
    top: -13rpx;
    right: -16rpx;
    border-radius: 0 0 50% 50%;
    z-index: 1;
    border: 1px solid #231F20;
}

.part .part_l .part_middle {
    width: 100%;
    height: fit-content;
    align-items: center;
    margin: auto 0;
}

.part .part_l .money {
    color: #231F20;
    font-size: 56rpx;
    line-height: 56rpx;
    font-weight: bold;
}

.part .part_l.only_price .money {
    height: fit-content;
    position: absolute;
    left: 50%;
    top: 50%;
    transform: translate(-50%, -50%);
}

.part .part_l .money text {
    font-size: 30rpx;
}

.part .part_l .condition {
    font-size: 22rpx;
    line-height: 22rpx;
    margin-top: 10rpx;
}

.part .part_l image {
    width: 128rpx;
    height: 128rpx;
    display: block;
    margin: auto;
}

.part .part_r {
    border-top: 1px solid #231F20;
    border-bottom: 1px solid #231F20;
    border-right: 1px solid #231F20;
    position: relative;
    display: inline-flex;
    width: calc(72.5% - 7rpx);
    min-height: 178rpx;
}

.part .part_r::after {
    content: '';
    width: 26rpx;
    height: 26rpx;
    background: #ffffff;
    position: absolute;
    bottom: -13rpx;
    left: -16rpx;
    border-radius: 50% 50% 0 0;
    z-index: 1;
    border: 1px solid #231F20;
}

.part .part_r .info {
    padding: 44rpx 39rpx;
    width: calc(100% - 170rpx);
}

.part .part_r .info .title {
    width: 100%;
    color: #231F20;
    font-size: 30rpx;
    line-height: 30rpx;
    font-weight: bold;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    word-break: break-all;
}

.desc {
    width: 100%;
    color: #7B7979;
    font-size: 24rpx;
    line-height: 30rpx;
    margin-top: 18rpx;
}

.desc image {
    width: 30rpx;
    vertical-align: middle;
}

.desc .arror_tranform {
    transform: rotate(180deg);
}

.intercept {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.part .part_r .btn {
    width: 24rpx;
    height: 100%;
    padding: 20rpx 38rpx;
    background: #231F20;
    color: #ffffff;
    text-align: center;
    font-size: 24rpx;
    line-height: 40rpx;
    display: table;
}

.part .part_r .btn view {
    vertical-align: middle;
    display: table-cell;
}








