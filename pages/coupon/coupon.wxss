page {
    background: #ffffff;
}

.list {
    width: 100%;
    padding: 30rpx 0 34rpx;
    margin-top: 80rpx;
    overflow-x: hidden;
}

.part {
    margin: 20rpx 30rpx;
    width: calc(100% - 60rpx);
    display: flex;
    overflow: hidden;
    position: relative;
}

