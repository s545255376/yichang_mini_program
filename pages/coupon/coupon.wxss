.poster {
    position: fixed;
    left: calc(50% - 300rpx);
    top: calc(50% - 481.5rpx);
    width: 600rpx;
    height: 857rpx;
    background: #ffffff;
    border-radius: 8rpx;
    z-index: 999;
}

.poster .close {
    width: 56rpx;
    position: absolute;
    left: calc(50% - 28rpx);
    bottom: -106rpx;
    z-index: 999;
}

.poster .head {
    width: calc(100% - 80rpx);
    display: flex;
    padding: 40rpx;
    border-bottom: 1px solid #EAEBEE;
}

.poster .head image {
    width: 168rpx;
    height: 168rpx;
}

.poster .head view {
    width: calc(100% - 185rpx);
    font-size: 28rpx;
    line-height: 50rpx;
    color: #000000;
    margin-left: 17rpx;
    text-align: justify;
}

.poster .code {
    width: 334rpx;
    margin: 55rpx 133rpx;
}

.poster .instructions {
    font-size: 26rpx;
    line-height: 26rpx;
    color: #000000;
    text-align: center;
}

.poster .instructions_info {
    width: 424rpx;
    margin: 20rpx 88rpx 0;
    font-size: 22rpx;
    line-height: 30rpx;
    color: #B7B7B7;
    text-align: center;
}

/* 不可用 */
.list.unclick .part::after {
    content: '';
    width: 100%;
    height: 100%;
    background: #FCFCFC;
    position: absolute;
    top: 0;
    left: 0;
    opacity: 0.8;
    z-index: 2;
}