    <!-- 拼团相关 -->
    <pop bindeventclose="closeToast" popname="grouponToast" wx:if="{{grouponToast}}">
        <view id="pop_custome_main">
            <view class="register_item">
                <view class="item_title">* 卡号：</view>
                <input type="number" placeholder="请输入卡号" bindinput="inputchange" data-name="card" />
            </view>
            <view class="register_item">
                <view class="item_title">* 卡密：</view>
                <input type="number" placeholder="请输入卡密" bindinput="inputchange" data-name="secret" />
            </view>
            <view class="buttonGroup" bindtap="goActive">
                <view>激活</view>
            </view>
        </view>
    </pop>
    <view class="part server">
        <view class="part_title">
            <text>其他功能</text>
        </view>
        <view class="part_body">
            <view bindtap="agreementList">
                <image src="../../images/mine/ic_agreement.png" mode="widthFix"></image>
                <view>我的协议</view>
            </view>
            <view bindtap="myAddress">
                <image src="../../images/mine/ic_location.png" mode="widthFix"></image>
                <view>收货地址</view>
            </view>
            <view bindtap="messageList">
                <image src="../../images/mine/ic_news.png" mode="widthFix"></image>
                <view>消息</view>
                <text wx:if="{{message != 0}}">{{message}}</text>
            </view>
            <view class="contact">
                <button open-type="contact" bindcontact="handleContact" class="contact_button"></button>
                <image src="../../images/mine/ic_service.png" mode="widthFix"></image>
                <view>联系客服</view>
            </view>
            <view bindtap="feedBack">
                <image src="../../images/mine/ic_feedback.png" mode="widthFix"></image>
                <view>意见反馈</view>
            </view>
            <!-- <view bindtap="questionBtn"><image src="../../images/mine/ic_question.png" mode="widthFix"></image><view>常见问题</view></view> -->
            <view bindtap="logoutBtn" wx:if="{{logType == true}}">
                <image src="../../images/mine/ic_signout.png" mode="widthFix"></image>
                <view>退出登录</view>
            </view>
            <view bindtap="moreFun" wx:if="{{logType == true}}">
                <image src="../../images/mine/ic_more.png" mode="widthFix"></image>
                <view>更多</view>
            </view>
        </view>
    </view>
</view>
<view wx:if="{{showActionsheet == true}}">
    <view class="mask" bindtap="logoutCancel"></view>
    <view class="actionSheet">
        <view class="btnlist">
            <!-- <view bindtap="logoutBtn">退出账号</view> -->
            <view bindtap="logoutBtn">注销账号</view>
        </view>
        <view class="cancel" bindtap="logoutCancel">取消</view>
    </view>
</view>
