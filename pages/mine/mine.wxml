<view class="{{pageShow == true?'wholepage':''}}">
    <view hidden="{{cancel_code}}" class="code" bindtap="cancelCode">
    <view class="code_box">
        <view class="code_text">出示会员二维码扫一扫</view>
        <!-- <canvas class="my_qrcode" style="width: 200px; height: 200px;" canvas-id="myQrcode"></canvas> -->
        <image class="my_qrcode" src="{{qrcode}}"></image>
    </view>
    </view>
    <view class="mine">
        <image src="../../images/mine/ic_mine_bg.png" mode="widthFix" class="mine_bg"></image>
        <view class="user">
            <image src="../../images/logo.png" mode="widthFix" class="logoavatar"></image>
            <!-- <image wx:else src="{{userinfo.portrait}}" mode="aspectFill" class="avatar"></image> -->
            <view class="user_info">
                <view wx:if="{{logType == false}}" bindtap="clicklogin" class="username">{{userinfo.username}}</view>
                <view wx:else class="username">{{userinfo.username}}</view>
                <view class="store_name">
                    <image src="../../images/mine/ic_myshopn.png" mode="widthFix"></image>我的余额:
                    {{money}}
                </view>
                <view class="store_name">
                    <image src="../../images/mine/ic_myshopn.png" mode="widthFix"></image>我的积分:
                    {{point}}
                </view>
                <!-- <view class="store_name">
                    <image src="../../images/mine/ic_myshopn.png" mode="widthFix"></image>我的卡券:
                    {{card_money}}
                </view> -->
            </view>
            <image wx:if="{{logType == true}}" class="my_code" src="../../images/mine/code.png" bindtap="showCode"></image>
        </view>
        <view class="card">
            <image src="../../images/mine/ic_mine_cardbg.png" mode="aspectFill" class="mine_bg"></image>
            <view class="mine_tips">
                <image src="../../images/mine/ic_vlog.png" mode="widthFix"></image> 
                <view wx:if="{{!is_vip}}">夷畅岩茶</view>
                <view wx:else>夷畅茶楼一楼免费喝茶</view>
            </view>
            
        </view>
        <!-- <view class="ic_writeoff" bindtap="ScanBtn" wx:if="{{userinfo.role_id == 1}}">
            <image src="../../images/mine/ic_writeoff.png" mode="widthFix"></image>
        </view> -->
    </view>
    <!-- 积分/优惠券 -->
    <!-- <view class="integral_coupon">
    <view class="border_r" bindtap="myScores"><view class="num">{{score}}</view><view class="name">积分</view></view>
    <view class="coupon" bindtap="myCoupon"><view class="num">{{card}}<view>NEW</view></view><view class="name">优惠券</view></view>
  </view> -->
    <view class="part">
        <view class="part_title">
            <text>我的订单</text>
            <view bindtap="goOrderList" data-type="0">
                查看全部订单
                <image src="../../images/ic_arrow_r.png" mode="widthFix"></image>
            </view>
        </view>
        <view class="part_body">
            <view bindtap="goOrderList" data-type="1">
                <image src="../../images/mine/ic_order_waitpay.png" mode="widthFix"></image>
                <view>待付款</view>
                <text wx:if="{{orderList.stayPaymentStats != 0}}">{{orderList.stayPaymentStats < 100 ? orderList.stayPaymentStats : '···' }}</text>
            </view>
            <view bindtap="goOrderList" data-type="2">
                <image src="../../images/mine/ic_order_waitsend.png" mode="widthFix"></image>
                <view>待发货</view>
                <text wx:if="{{orderList.stayDeliverStats != 0}}">{{orderList.stayDeliverStats < 100 ? orderList.stayDeliverStats : '···' }}</text>
            </view>
            <view bindtap="goOrderList" data-type="3">
                <image src="../../images/mine/ic_order_waitget.png" mode="widthFix"></image>
                <view>待收货</view>
                <text wx:if="{{orderList.stayReceivStats != 0}}">{{orderList.stayReceivStats < 100 ? orderList.stayReceivStats : '···' }}</text>
            </view>
            <view bindtap="goOrderList" data-type="4">
                <image src="../../images/mine/ic_order_finish.png" mode="widthFix"></image>
                <view>已完成</view>
                <text wx:if="{{orderList.stayCompleteStats != 0}}">{{orderList.stayCompleteStats < 100 ? orderList.stayCompleteStats : '···' }}</text>
            </view>
            <view bindtap="refundList">
                <image src="../../images/mine/ic_order_returnback.png" mode="widthFix"></image>
                <view>退款/退货</view>
                <text wx:if="{{orderList.stayRefundStats != 0}}">{{orderList.stayRefundStats < 100 ? orderList.stayRefundStats : '···' }}</text>
            </view>
        </view>
    </view>
    <view class="part server">
        <view class="part_title">
            <text>我的服务</text>
        </view>
        <view class="part_body">
            <view bindtap="buyPoints">
                <image src="../../images/mine/vip.png" mode="widthFix"></image>
                <view>会员充值</view>
            </view>

            <view bindtap="active">
                <image src="../../images/mine/card_secret.png" mode="widthFix"></image>
                <view>激活卡密</view>
            </view>

            <view bindtap="scanPoints" wx:if="{{store > 0}}">
                <image src="../../images/mine/scan.png" mode="widthFix"></image>
                <view>门店核销</view>
            </view>
            <!-- <view bindtap="goDetail">
                <image src="../../images/mine/recharge.png" mode="widthFix"></image>
                <view>余额明细</view>
                <text wx:if="{{orderList.stayCompleteStats != 0}}">{{orderList.stayCompleteStats < 100 ? orderList.stayCompleteStats : '···' }}</text>
            </view> -->
            <view bindtap="myCoupon">
                <image src="../../images/mine/ic_card.png" mode="widthFix"></image>
                <view>我的电子卡</view>
            </view>
            <view bindtap="goDetails">
                <image src="../../images/mine/search.png" mode="widthFix"></image>
                <view>消费明细</view>
            </view>
            <view bindtap="goMyTeam">
                <image src="../../images/mine/friend.png" mode="widthFix"></image>
                <view>我的亲友</view>
            </view>
        </view>
    </view>
