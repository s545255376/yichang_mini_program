/* pages/classifySearch/classifySearch.wxss */
#search .van-search {
    border-bottom:1px solid #EDEDED;
}
#classifySearch {
    width: 100%;
    padding: 30rpx;
    margin-top: 30rpx;
    box-sizing: border-box;
    display: flex;
    flex-wrap: wrap;
    align-items: flex-start;
    background-color: #fff;
}
.goodspart:nth-child(odd) {
    width:calc(50% - 15rpx) !important;
    margin-right:28rpx !important;
}
.goodspart:nth-child(2n) {
    width:calc(50% - 15rpx) !important;
    margin-right: 0 !important;
}
.goodspart .image image {
    box-sizing: border-box;
}
#search {
    position: fixed;
    top:0;
    left:0;
    width:100%;
    z-index: 1;
}
.ic_empty {
    position: relative;
    margin-top: 0;
    padding: 30rpx 0;
}
.goodspart .title {
    overflow: unset !important;
    white-space: unset !important;
    line-height: unset;
    height: unset;
}