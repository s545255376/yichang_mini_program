.bk_img {
    position: fixed;
    top: 0;
    z-index: -1;
    height: 100%;
    width: 100%;
}
.part {
    color: #ffffff;
}
.header {
    border: solid;
    margin-top: 3%;
    margin-left: 3%;
    width: 94%;
    display: flex;
    flex-direction: row;
    justify-content: space-around;
}
.header_box {
    margin-top: 5%;
    display: flex;
    flex-direction: column;
    margin-bottom: 5%;
}
.history {
    margin-top: 4%;
    height: 1200rpx;
}
.body {
    display: flex;
    flex-direction: row;
    justify-content: space-around;
    margin-top: 4%;
    height: 80rpx;
    line-height: 80rpx;
    border-bottom-style: dotted;
    width: 94%;
    margin-left: 3%;
}
.none {
    text-align: center;
    margin-top: 30%;
}