/* 拼团玩法 */
#groupon_slot {
    margin-top:18rpx;
    padding-bottom:30rpx;
    width:100%;
    background: #fff;
}
#groupon_slot #title {
    width: calc(100% - 60rpx);
    padding: 0 30rpx;
    font-size: 30rpx;
    color: #231F20;
    line-height: 100rpx;
}
#groupon_slot #content {
    padding:0 30rpx;
    width:calc(100% - 60rpx);
}
#howtoPlay {
    background: rgba(35, 31, 32, 0.04);
    padding:30rpx;
    box-sizing: border-box;
}
#step_simple {
    display: flex;
    justify-content: space-between;
    align-items: center;
    font-size:26rpx;
}
#step_simple > view {
    width:33%;
    display: flex;
    align-items: center;
    position: relative;
}
#step_simple > view:nth-child(1) {
    justify-content: flex-start;
}
#step_simple > view:nth-child(2) {
    justify-content: center;
}
#step_simple > view:nth-child(3) {
    justify-content: flex-end;
}
#step_simple > view:nth-child(2)::before {
    content: '>';
    position: absolute;
    left:-20rpx;
}
#step_simple > view:nth-child(2):after {
    content: '>';
    position: absolute;
    right:-20rpx;
}
#step_simple > view > view {
    width:40rpx;
    height:40rpx;
    border-radius: 50%;
    background: #231F20;
    color: #fff;
    display: flex;
    justify-content: center;
    align-items: center;
    font-size:24rpx;
    margin-right:10rpx;
}
#step_detailed {
    margin-top:20rpx;
    box-sizing: border-box;
    font-size:28rpx;
    position: relative;
    overflow: hidden;
}
#step_detailed view {
    margin-bottom:10rpx;
    color: rgba(167, 165, 166, 1);
}
#step_detailed view:last-child {
    margin-bottom:0;
}
#step_detailed.blockHide {
    word-break: break-word;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 5;
    -webkit-box-orient: vertical;
}
#groupon_slot .slotShow, #groupon_slot .slotHide {
    width:100%;
    display: flex;
    justify-content: center;
    align-items: center;
    background: linear-gradient(180deg, rgba(255, 255, 255, 0) 0%, rgba(246, 246, 246, 1) 50%);
    color: rgba(35, 31, 32, 1);
    position: absolute;
    bottom:0;
    font-size:24rpx;
    padding-top:50rpx;
}
#groupon_slot .slotShow image, #groupon_slot .slotHide image {
    width:15rpx;
    height:15rpx;
    margin-left:10rpx;
    transform: rotate(180deg);
}
#groupon_slot .slotHide image {
    transform: rotate(0deg);
}
#groupon_slot .slotHide {
    align-items: center !important;
    padding-top:10rpx !important;
    position: unset !important;
}
